let mut output: String = String::new();
    match msg.read_until(b'\n', &mut buffer) {
        Err(_) => panic!("Reading Failed"),
        _ => {}
    }

    
    match from_utf8(&buffer) {
        Ok(s) => {
            output.push_str(s);
            return output;
        }
        Err(e) => {
            panic!("Buffer invalid: {}", e);
        }
    }